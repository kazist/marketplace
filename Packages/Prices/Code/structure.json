    "query": {
        "from": {
            "table": "Marketplace\\Packages\\Prices\\Code\\Tables\\Prices",
            "alias": "mpp",
            "name": "#__marketplace_packages_prices"
        },
        "select": [
            "mpp.id",
            "mpp.price",
            "mpp.package_id",
            "mp.title AS package_id_title",
            "mpp.no_of_days",
            "mpp.featured",
            "mpp.published",
            "mpp.created_by",
            "uu.name AS created_by_name",
            "mpp.date_created",
            "mpp.modified_by",
            "uu_287.name AS modified_by_name",
            "mpp.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Marketplace\\Packages\\Code\\Tables\\Packages",
                "alias": "mp",
                "name": "#__marketplace_packages",
                "on": "mpp.package_id = mp.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "mpp.created_by = uu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_287",
                "name": "#__users_users",
                "on": "mpp.modified_by = uu_287.id"
            }
        ]
    }
