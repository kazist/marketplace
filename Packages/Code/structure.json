    "query": {
        "from": {
            "table": "Marketplace\\Packages\\Code\\Tables\\Packages",
            "alias": "mp",
            "name": "#__marketplace_packages"
        },
        "select": [
            "mp.id",
            "mp.title",
            "mp.description",
            "mp.has_currency",
            "mp.has_price",
            "mp.has_discount",
            "mp.published",
            "mp.created_by",
            "uu.name AS created_by_name",
            "mp.date_created",
            "mp.modified_by",
            "uu_8849.name AS modified_by_name",
            "mp.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "mp.created_by = uu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_8849",
                "name": "#__users_users",
                "on": "mp.modified_by = uu_8849.id"
            }
        ]
    }
