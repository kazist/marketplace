    "query": {
        "from": {
            "table": "Marketplace\\Adverts\\Images\\Code\\Tables\\Images",
            "alias": "mai",
            "name": "#__marketplace_adverts_images"
        },
        "select": [
            "mai.id",
            "mai.advert_id",
            "ma.title AS advert_id_title",
            "mai.image",
            "mm.title AS image_title",
            "mm.file AS image_file",
            "mai.created_by",
            "mai.date_created",
            "mai.modified_by",
            "mai.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Marketplace\\Adverts\\Code\\Tables\\Adverts",
                "alias": "ma",
                "name": "#__marketplace_adverts",
                "on": "mai.advert_id = ma.id"
            },
            {
                "table": "Media\\Media\\Code\\Tables\\Media",
                "alias": "mm",
                "name": "#__media_media",
                "on": "mai.image = mm.id"
            }
        ]
    }
